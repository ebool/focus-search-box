{
  "name": "focus search box",
  "version": "1.0",
  "description": "chrome extension project for focusing on search box",
  "permissions": ["storage", "declarativeContent", "activeTab", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "browser_action": {
    "default_icon": {
      "16": "images/icon1x.png",
      "32": "images/icon2x.png",
      "48": "images/icon3x.png",
      "128": "images/icon4x.png"
    }
  },
  "icons": {
    "16": "images/icon1x.png",
    "32": "images/icon2x.png",
    "48": "images/icon3x.png",
    "128": "images/icon4x.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
